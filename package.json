{
  "name": "hackeruna-frontend",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rm -rf dist .angular",
    "build": "npm run clean && ng build && node copy-htaccess.js",
    "build:prod": "npm run clean && ng build --configuration production && node copy-htaccess.js",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "private": true,
